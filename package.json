{
  "name": "@typecomposer/typecomposer",
  "version": "0.1.50",
  "description": "Build modern web interfaces without touching a single line of HTML",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "./dist/global/index.js",
    "./dist/core/styles/**"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TypeComposer/typecomposer.git"
  },
  "keywords": [
    "typecomposer",
    "web",
    "ui",
    "framework",
    "front-end"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run minify",
    "minify": "find dist -name '*.js' -exec terser {} --compress drop_console=true --mangle --output {} \\;",
    "pub": "npm run build && npm publish --access public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.6.1",
    "csstype": "^3.1.3",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
