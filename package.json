{
  "name": "typecomposer",
  "version": "0.1.48",
  "repository": {
    "type": "git",
    "url": "https://github.com/zico15"
  },
  "description": "A component library for getting people started with easy to re-use components for everyday projects.",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "sideEffects": [
    "./global/index.js",
    "./core/styles/**"
  ],
  "files": [
    "README.md",
    "core",
    "global",
    "styles",
    "index.js",
    "index.d.ts",
    "index.js.map",
    "client.d.ts",
    "typings/index.d.ts",
    "public",
    "assets",
    "translation",
    "LICENSE"
  ],
  "scripts": {
    "copy": "rm -rf '/Users/Ezequiel/Desktop/learn-typecomposer-main/node_modules/typecomposer/* -y' && cp -r ./dist/* '/Users/Ezequiel/Desktop/learn-typecomposer-main/node_modules/typecomposer'",
    "build": "npm run build:esm && npm run copy",
    "build:esm": "tsc -p tsconfig.json",
    "minify": "find dist -name '*.js' -exec terser {} --compress drop_console=true --mangle --output {} \\;",
    "pub": "npm run build && cd dist && npm publish"
  },
  "keywords": [
    "typecomposer"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.js"
    }
  },
  "preferGlobal": true,
  "author": "Ezequiel",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.3",
    "csstype": "^3.1.3",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}
